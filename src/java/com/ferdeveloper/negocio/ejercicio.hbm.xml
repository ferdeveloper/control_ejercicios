<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.ferdeveloper.negocio.Ejercicio" table="ejercicios">
    <id column="idEjercicio" name="idEjercicio" type="integer"/>
    <property name="nombreEjercicio" column="nombreEjercicio"/>
    <property name="descripcionEjercicio" column="descripcionEjercicio"/>
    <property name="categoriaEjercicio" column="categoriaEjercicio"/>
    <property name="cursoEjercicio" column="cursoEjercicio"/>
    <property name="fechaCreacion" column="fechaCreacion"/>
    <property name="fechaEjercicio" column="fechaEjercicio"/>
    <property name="observacionesEjercicio" column="observacionesEjercicio"/>
</class>
</hibernate-mapping>